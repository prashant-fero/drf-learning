#!/home/ubuntu/Learning/Fero/env/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'celery-flower==1.0.1','console_scripts','flower'
__requires__ = 'celery-flower==1.0.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('celery-flower==1.0.1', 'console_scripts', 'flower')()
    )
